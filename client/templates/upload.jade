template(name='upload')
  .container.uploadPage
    .row
      .col-md-offset-4.col-md-4
        img.img-responsive.erlogo(src="data:image/gif;base64,")
        h1 {{_ "Share the moment"}}
    .row
      .col-md-offset-4.col-md-4
        if currentUser
          .form-group.fileupload(class="{{#if error 'url'}}has-error{{/if}}")
            input.form-control(type="file" id="uploadBtn" name="uploadPic" accept="image/*")
            span.error-msg {{error "url"}}
          .form-group.username(class="{{#if error 'name'}}has-error{{/if}}")
            input.form-control(type="text" id="uploadUser" name="uploader" placeholder='{{_ "Your Name"}}' value="{{username}}")
            span.error-msg {{error "name"}}
          .form-group
            button.submit.btn.btn-primary.btn-lg.btn-block.upload-btn(type="button") {{_ "Submit"}}
          .karma {{uploadedCount}} {{_ "uploaded"}}
        else
          h3 Crown source wedding photos
          p See what's live in the event
        +loginButtons


